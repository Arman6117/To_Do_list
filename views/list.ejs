
   <%- include("header") -%>

    <div class="box" id="heading">
      <h1><%= listTitle%></h1>
    </div>

    <div class="box">
      <% for (let i = 0; i < new_task.length; i++) { %>
      <div class="item">
        <input type="checkbox" />
        <p><%= new_task[i]%></p>
      </div>
      <% } %>
   

    <form action="/" method="post" class="item">
      
      <input type="text" placeholder="Add a new task" name="new_task" autocomplete="off" />
      <button type="submit" name="list" value=<%= listTitle %></button>+</button>
    </form>
  </div>

  <%- include("footer") -%>